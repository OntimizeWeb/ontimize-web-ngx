  "scripts": {
    "preinstall": "npm config set package-lock false",
    "rimraf": "rimraf",
    "test": "karma start",
    "clean": "rimraf dist tmp",
    "init-common": "npm run clean && gulp inline-templates && gulp copy-table-files-tmp",
    "end-common": "gulp copy-files && gulp ontimize.styles",
    "webpack:dev": "npm run init-common && webpack --config config/webpack.dev.js --progress && npm run end-common",
    "webpack:prod": "npm run common && webpack --config config/webpack.prod.js --progress && npm run end-common",
    "aot": "npm run common && node_modules/.bin/ngc -p tsconfig.aot.json && npm run end-common"
  },
