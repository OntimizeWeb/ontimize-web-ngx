{
  "name": "ontimize-web-ngx",
  "version": "15.0.0-SNAPSHOT-4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run build-theme && npm run build-styles && npm run copy-files && npm run copy-files-themes",
    "build-prod": "ng build -c production && npm run build-theme && npm run build-styles && npm run copy-files && npm run copy-files-themes",
    "build-theme": "bundle-scss --config",
    "build-styles": "scss-bundle -c scss-bundle-ontimize.config.json",
    "copy-files": "copyfiles -u 3 ./projects/ontimize-web-ngx/assets/svg/ontimize-icon-set.svg ./dist/assets && echo '----Â¡Assets copied!----'  ",
    "copy-files-themes": "npm run clean && gulp copy-files",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --no-watch --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rimraf": "rimraf",
    "clean": "rimraf tmp"
  },
  "bundleStyle": {
    "dest": "./dist/theme.scss",
    "mask": [
      "./projects/ontimize-web-ngx/src/lib/components/**/*theme.scss",
      "./projects/ontimize-web-ngx/src/lib/shared/**/*theme.scss",
      "./projects/ontimize-web-ngx/src/lib/layouts/**/*theme.scss"
    ],
    "sort": [
      "o-material"
    ]
  },
  "private": true,
  "dependencies": {
